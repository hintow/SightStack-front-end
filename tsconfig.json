{
    "compilerOptions": {
      "jsx": "react-jsx", 
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "strict": true,
      "target": "es5",
      "lib": ["dom", "es2015"],
      "module": "esnext",
      "baseUrl": "./src"
    },
    "include": ["src"]
  }