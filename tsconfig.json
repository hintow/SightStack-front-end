{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es2015"],
      "jsx": "react-jsx",
      "module": "esnext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "skipLibCheck": true,
      "strict": true,
      "baseUrl": "./src"
    },
    "include": ["src", "vite.config.ts"]
  }